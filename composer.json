{
    "name": "miladimos/conf",
    "description": "a dynamic key:value json config file",
    "homepage": "https://github.com/miladimos/conf",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel config",
        "lumen packages",
        "laravel packages",
        "json config",
        "lumen support"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "message.tome1331@gmail.com",
            "role": "maintaner",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Miladimos\\Conf\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Miladimos\\Conf\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miladimos\\Conf\\Providers\\ConfServiceProvider"
            ],
            "aliases": {
                "Conf": "Miladimos\\Conf\\Facades\\ConfFacade"
            }
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
